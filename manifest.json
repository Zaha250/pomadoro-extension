{
  "manifest_version": 3,
  "name": "Pomodoro",
  "version": "1.0",
  "description": "Расширение для тайм-менеджмента",
  "permissions": ["scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["src/index.js"],
      "matches": ["<all_urls>"]
    }
  ]
}